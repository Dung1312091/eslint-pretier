{
    "plugins": [
        "prettier"
    ],
    "extends": [
        "plugin:prettier/recommended"
    ],
    "rules": {
        "prettier/prettier": ["error"]
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "es6": true,
        "node": true

    },
    "globals": {
        "arguments": true
    }
}